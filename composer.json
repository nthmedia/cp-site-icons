{
    "name": "nthmedia/cp-site-icons",
    "description": "Display site icon when editing entries",
    "type": "craft-plugin",
    "version": "1.0.6",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "cp site icons"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "NTH media",
            "homepage": "https://nthmedia.nl"
        }
    ],
    "require": {
        "php": "^8.0",
        "craftcms/cms": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
          "nthmedia\\cpsiteicons\\": "src/"
        }
    },
    "extra": {
        "name": "CP Site Icons",
        "handle": "cp-site-icons",
        "developer": "NTH media",
        "developerUrl": "https://nthmedia.nl",
        "class": "nthmedia\\cpsiteicons\\CpSiteIcons"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "craftcms/rector": "dev-main",
        "craftcms/ecs": "dev-main",
        "phpstan/phpstan": "^1.6"
    }
}
